/**
 * 
 */
package com.scotmatson.banking;

public class Account {
	
	private String type;
	private int number;
	private int balance;
	
	
	public Account (String type, int number) {
		this.type = type;
		this.number = number;
		this.balance = 0;
	}
	
	/**
	 * 
	 * @param type
	 * @param number
	 * @param balance
	 */
	public Account (String type, int number, int balance) {
		this.type = type;
		
	}
	
	/**
	 * 
	 * @return the account type.
	 */
	public String getType()
	{
		return this.type;
	}
	
	/**
	 * 
	 * @return the account number.
	 */
	public int getNumber()
	{
		return this.number;
	}
	
	/**
	 * 
	 * @return the account balance.
	 */
	public int getBalance()
	{
		return this.balance;
	}
	
}
