package chapter04;

import javax.swing.JOptionPane;

public class Example002 {

	public static void main(String[] args) {
		
		JOptionPane.showMessageDialog(
				null, //parent window
				"Hello World!", //message
				"Message", // window title
				JOptionPane.INFORMATION_MESSAGE, // message type
				new CarIcon(20);
				);

	}

}
