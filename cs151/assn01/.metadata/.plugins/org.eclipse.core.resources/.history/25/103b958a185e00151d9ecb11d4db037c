/**
 * 
 */
package com.scotmatson.banking;

public class ATMSimulator implements Runnable
{
	/**
	 * 
	 */
	@Override
	public void run() 
	{
		System.out.println("Inside Run");
	}


	/**
	 * 
	 * @param args
	 */
	public static void main(String[] args) 
	{
		(new Thread(new ATMSimulator())).start();
		System.out.println("Outside Run");
	}


}
